    "brightecapital/sqs": "^1.0.0",
    "brightecapital/sns": "^1.0.0",
    "brightecapital/service-schema": "^1.2",
    "doctrine/orm": "^2.5",
    "sentry/sentry": "1.10.0",
    "monolog/monolog": "1.25.1",
    "predis/predis": "^1.1",
    "guzzlehttp/guzzle": "^6.3",
    "firebase/php-jwt": "^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.1"
